{
    "manifest_version": 2,
    "name": "Ywayy",
    "version": "0.1-beta",
    "content_scripts": [
        {
            "matches" : [
                "<all_urls>"
            ],
            "js" : [
                "content.js"
            ]
        }
    ],
    "permissions": [
        "contextMenus"
    ],
    "background": {
        "scripts": [
            "rabbit.min.js",
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": "icon.png"
    }
}