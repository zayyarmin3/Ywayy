{
    "manifest_version": 2,
    "name": "Ywayy",
    "version": "1.0",
    "content_scripts": [
        {
            "matches" : [
                "<all_urls>"
            ],
            "js" : [
                "content.js"
            ]
        }
    ],
    "commands" : {
        "activate_ywayy": {
            "suggested_key": {
              "default": "Ctrl+Y",
              "mac": "Command+Y"
            },
            "description": "Activate Yway"
        }
    },
    "permissions": [
        "contextMenus"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "default_icon": "icon.png"
    },
    "icons": {
        "128" : "icon.png"
    }
}